<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.barbara.newspapersub">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:name=".Contents.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/newspaper"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light">
        <activity android:name=".Actitity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Actitity.user.UserActivity"/>
        <activity android:name=".Actitity.BaseActivity"/>
        <activity android:name=".Actitity.user.UpdateUserActivity"/>
        <activity android:name=".Actitity.user.RegisterActivity"/>
        <activity android:name=".Actitity.admin.AdminActivity"/>
        <activity android:name=".Actitity.user.UpdatePwdActivity"/>
        <activity android:name=".Actitity.admin.AddUserActivity"/>
        <activity android:name=".Actitity.admin.AddNewsActivity"/>
        <activity android:name=".Actitity.user.BuyActivity"/>
        <activity android:name=".Actitity.admin.SelectByUserActivity"/>
        <activity android:name=".Actitity.admin.SelectByNewsActivity"/>
        <activity android:name=".Actitity.admin.SelectByDepartActivity"/>

    </application>

</manifest>
